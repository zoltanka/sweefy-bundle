{
    "name": "zfekete/sweefy",
    "description": "Light-weight hydration build on doctrine DBAL.",
    "type": "symfony-bundle",
    "autoload": {
        "psr-4": {
            "ZFekete\\Sweefy\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "ZFekete\\Sweefy\\Tests": "tests/"
        }
    },
    "authors": [
        {
            "name": "Z.Fekete",
            "email": "fekzol.13ker@gmail.com"
        }
    ],
    "require": {
        "php": ">8.1",
        "doctrine/dbal": "^3.3",
        "symfony/dependency-injection": "~6.0.0",
        "symfony/http-kernel": "~6.0.0",
        "symfony/config": "~6.0.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "6.0.*"
        }
    }
}
